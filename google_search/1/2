<script type="text/javascript">
var bLoginUser = false;
var gsUserblogId="";
var gsLocationOrigin = "http://m.blog.naver.com";
var loginCurrentFullUrl ="";
var logoutCurrentFullUrl = "";
var gbQuickMenuOpen = false;

var CommonQuickMenuLayerController = $Class({
	$init : function(){
	},
	
	start : function(){
		this.paramUtil = new ParamUtil();
		this._setEvent();
		this._getTodayVisit();
		
	},
	
	_setEvent : function(){
		$Fn(this._doClick, this).attach($$('.btn_menu'), "click");
		$Fn(this._doClick, this).attach($$('._btn_close_qmenu'), "click");
	},
	
	_doClick : function(oEvent){
		var wel = oEvent.getEventElement();
		
		if (wel.hasClass("_btn_menu")) {
			this._goRedirectUrl(paramUtil.getParam(wel.className()));	
		} else if (wel.hasClass("_btn_close_qmenu")) {
			gbQuickMenuOpen = false;
			$Element("btn_q").removeClass("on");
			$Element($$.getSingle("._header")).removeClass("on");
			$Element("ct").removeClass("on");
			$Element("quick_menu").removeClass("on");
			$Element("body").removeClass("on");
			var welFooter = $Element($$.getSingle("._commonFooter"));
			if (welFooter) {
				welFooter.removeClass("on");
			}
			// 개별블로그 메인에만 cover가 존재함
			if ($Element("cover")) {
				$Element("cover").removeClass("on");
			}
		}
		
		if (wel.hasClass('_returnFalse')) {
	    	oEvent.stop();
	    }
	},
	
	_getTodayVisit : function() {
		if (gsUserblogId == "") {
			return;
		} 
		
		if (window.innerWidth < 786 && window.innerHeight < 786) {
			return;
		}
		
		var param = {'blogId' : gsUserblogId};
		var oAjax = $Ajax("/QuickMenuAsync.nhn", {
			onload: $Fn(function(o){
				var oResponse = $Json(o.json()).toObject();
				if (oResponse.result) {
					$("todayVisit").innerHTML = "Today " + oResponse.result.visitCount;
					return;
				}
				
			}, this).bind()
		});
		oAjax.request(param);
	},
	
	_goRedirectUrl : function(params){
		if(params.length){
			if(params[0] !== "/Index.nhn" && !bLoginUser ){
				loginCurrentFullUrl = gsLocationOrigin + params[0];
				loginUtil.login();
				return;
			}
			location.href=params[0];
		}
	}
	
});

var commonQuickMenuLayerController = new CommonQuickMenuLayerController();
commonQuickMenuLayerController.start();
</script>

